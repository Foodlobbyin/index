{
  "name": "foodlobbyin-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "predev": "node -e \"const fs = require('fs'); const path = require('path'); const checkPaths = ['node_modules', path.join('..', 'node_modules')]; const hasNodeModules = checkPaths.some(p => fs.existsSync(p)); if (!hasNodeModules) { console.error('\\n‚ùå ERROR: Dependencies not installed!\\n\\nPlease run from project root: npm install\\n'); process.exit(1); }\"",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "axios": "^1.5.0",
    "lucide-react": "^0.574.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.24",
    "jsdom": "^22.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^4.9.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.6"
  }
}
